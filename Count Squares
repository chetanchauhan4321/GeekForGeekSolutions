http://practice.geeksforgeeks.org/problems/count-squares/0

Solution C++;
#include <iostream>
using namespace std;

int main() {
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        int x;
        cin>>x;
        int r=0;
        int result=0;
        while(result<x)
        {r++;
            result=r*r;
            
        }
        cout<<r-1<<endl;
    }
	//code
	return 0;
}
