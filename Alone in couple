link: http://practice.geeksforgeeks.org/problems/alone-in-couple/0

Solution c++:
#include <bits/stdc++.h>
using namespace std;

int main() {
	//code

	int n;
	cin>>n;
	for(int k=0;k<n;k++)
	{
	    int l;
	    cin>>l;
	    vector<int>h(1000,0);
	    for(int i=0;i<l;i++)
	    {
	        int x;
	        cin>>x;
	        h[x]=h[x]+1;
	    }
	    int i=0;
	    for(i=0;i<1000;i++)
	    {
	        if(h[i]%2!=0)
	        {
	        break;
	        }
	    }
	    cout<<i<<endl;
	    
	   
	    
	}
		return 0;
}
