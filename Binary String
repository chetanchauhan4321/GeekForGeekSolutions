#include <bits/stdc++.h>
using namespace std;

int main() {
  int t;
	cin>>t;
	for(int z=0;z<t;z++)
	{   int n;
	    cin>>n;
	    vector<int>h(2,0);
	    string s;
	    cin>>s;
	    for(int i=0;i<s.size();i++)h[s[i]-48]=h[s[i]-48]+1;
	    cout<<h[1]*(h[1]-1)/2<<endl;
	}
	return 0;
}
