http://practice.geeksforgeeks.org/problems/finding-the-numbers/1

#include <bits/stdc++.h>
using namespace std;

int main() {
	int z;
	cin>>z;
	for(int k=0;k<z;k++)
	{
	    int n;
	    cin>>n;
	    vector<long long>h(1000000,0);
	    for(int i=0;i<2*n+2;i++)
	    {
	        int x;
	        cin>>x;
	        h[x]=h[x]+1;
	    }
	    for(int i=0;i<h.size();i++)
	    {
	        if(h[i]==1||h[i]%2!=0)cout<<i<<" ";
	        
	    }
	   
	   //cout<<h.size();
	    cout<<endl;
	}
	return 0;
}
